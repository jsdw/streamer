{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "strictNullChecks": true,
        "alwaysStrict": true,
        "strict": true,
        "lib": ["es5", "es6", "dom"],
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true
    }
}